{
  "name": "pilltracker-dashboard",
  "version": "0.1.0",
  "description": "A dashboard application for trickTracker app\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001bweb application for pilltracker app",
  "main": "app.ts",
  "scripts": {
    "start": "npm run serve",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" \"npm run watch-ts\" \"npm run watch-node\"",
    "serve": "node build/app.js",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon build/app.js",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "./node_modules/jest/bin/jest.js --forceExit --coverage --verbose",
    "watch-test": "./node_modules/jest/bin/jest.js --forceExit --coverage --verbose --watch"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-validator": "~6.2.0",
    "express-winston": "~4.0.0",
    "helmet": "^3.21.2",
    "jsonschema": "1.2.4",
    "mongoose": "~5.6.9",
    "ramda": "^0.26.1",
    "winston": "^3.0.0"
  },
  "author": "Th3CracKed, naj147",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "~4.17.1",
    "@types/express-rate-limit": "^3.3.3",
    "@types/express-validator": "~3.0.0",
    "@types/express-winston": "~3.0.4",
    "@types/helmet": "0.0.44",
    "@types/jsonschema": "~1.1.1",
    "@types/mongoose": "~5.5.18",
    "@types/node": "~12.7.8",
    "@types/ramda": "~0.26.26",
    "concurrently": "~4.1.2",
    "jest": "~24.9.0",
    "nodemon": "~1.19.3",
    "tslint": "^5.20.0",
    "typescript": "~3.6.3"
  }
}
